{
  "manifest_version": 3,
  "name": "Tab Dashboard",
  "version": "1.0",
  "description": "View, manage, and organize all open tabs across all windows.",
  "permissions": [
    "tabs",
    "sessions",
    "storage",
    "scripting",
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["note-injector.js"]
    }
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "action": {
    "default_title": "Open Tab Dashboard"
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
  
}